{
    "name": "javiereguiluz/easy-admin-demo",
    "license": "MIT",
    "type": "project",
    "description": "A Symfony demo application to showcase the EasyAdmin bundle features.",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "minimum-stability": "beta",
    "repositories": [
        { "type": "git", "url": "https://github.com/javiereguiluz/VichUploaderBundle" },
        { "type": "git", "url": "https://github.com/javiereguiluz/IvoryCKEditorBundle" },
        { "type": "git", "url": "https://github.com/javiereguiluz/WhiteOctoberPagerfantaBundle" }
    ],
    "require": {
        "php": "^7.1",
        "ext-pdo_sqlite": "*",
        "egeloen/ckeditor-bundle": "dev-allow_sf4_components",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/asset": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/polyfill-apcu": "^1.6",
        "symfony/security-bundle": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/templating": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "vich/uploader-bundle": "dev-allow_sf4_components",
        "white-october/pagerfanta-bundle": "dev-allow_sf4_components"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "symfony/browser-kit": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<4.0",
        "symfony/debug": "<4.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
